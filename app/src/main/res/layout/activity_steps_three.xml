<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#fff"
    android:id="@+id/activity_step_three"
    >
    <android.support.v7.widget.CardView
        android:id="@+id/personalDetailsCard"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="12dp"
        app:cardCornerRadius="5dp"
        app:cardElevation="5dp">




    <TableLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_marginBottom="40dp"
        android:layout_height="match_parent"
        android:stretchColumns="1">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Post-Test Counseling"
            android:textColor="#000"
            android:textSize="20dp"
            android:textStyle="bold" />
        <TableRow android:padding="5dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"

                android:text="Post-test counseling plan"
                android:textColor="#000"
                android:textStyle="bold"

                />
        </TableRow>
        <TableRow
            android:padding="5dp"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="5dp"
                android:gravity="start"
                android:text="HIV Test Result"
                android:textColor="#000"
                />


        </TableRow>
        <android.support.v7.widget.AppCompatSpinner
            android:id="@+id/hivTestResult"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAlignment="center"
            android:layout_centerVertical="true"
            android:background="@drawable/bg_dropdown"
            android:layout_gravity="center"
            android:entries="@array/tested_result"
            android:gravity="center"
            android:spinnerMode="dropdown" />

        <TableRow
            android:padding="5dp"
            >

            <CheckBox
                android:id="@+id/artReferred"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Referred to ART Service"
                android:gravity="start" />


        </TableRow>



        <TableRow
            android:padding="5dp"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Tested for HIV before within the year"
                android:textColor="#000"
                />




        </TableRow>


        <android.support.v7.widget.AppCompatSpinner
            android:id="@+id/testedHiv"
            android:textAlignment="center"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_gravity="center"
            android:background="@drawable/bg_dropdown"
            android:gravity="center"
            android:entries="@array/oneAndTwo"
            android:spinnerMode="dropdown" />

        <android.support.v7.widget.AppCompatSpinner
            android:id="@+id/testedHiv2"
            android:textAlignment="center"
          android:layout_marginTop="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_gravity="center"
            android:background="@drawable/bg_dropdown"
            android:gravity="center"
            android:entries="@array/tested_resultx"
            android:spinnerMode="dropdown" />


        <TableRow
            android:padding="5dp"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="5dp"
                android:gravity="start"
                android:text="HIV Request and result form signed by tester(s)"
                android:textColor="#000"
                />

        </TableRow>

        <android.support.v7.widget.AppCompatSpinner
            android:id="@+id/postTest1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAlignment="center"
            android:layout_centerVertical="true"
            android:layout_gravity="center"
            android:background="@drawable/bg_dropdown"
            android:entries="@array/oneAndTwo"
            android:gravity="center"
            android:spinnerMode="dropdown" />
        <TableRow
            android:padding="5dp"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="5dp"
                android:gravity="start"
                android:text="HIV Request and result form filled with CT intake inform"
                android:textColor="#000"
                />


        </TableRow>
        <android.support.v7.widget.AppCompatSpinner
            android:id="@+id/postTest2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:textAlignment="center"
            android:background="@drawable/bg_dropdown"
            android:layout_gravity="center"
            android:entries="@array/oneAndTwo"
            android:gravity="center"
            android:spinnerMode="dropdown" />
        <TableRow
            android:padding="5dp"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="5dp"
                android:gravity="start"
                android:text="Client received HIV test result"
                android:textColor="#000"
                />


        </TableRow>

        <android.support.v7.widget.AppCompatSpinner
            android:id="@+id/postTest3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:textAlignment="center"
            android:layout_gravity="center"
            android:background="@drawable/bg_dropdown"
            android:entries="@array/oneAndTwo"
            android:gravity="center"
            android:spinnerMode="dropdown" />


        <TableRow
            android:padding="10dp"
            >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="5dp"
                android:gravity="start"
                android:text="Post Test Counseling done"
                android:textColor="#000" />



        </TableRow>



        <android.support.v7.widget.AppCompatSpinner
            android:id="@+id/postTest4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:textAlignment="center"
            android:layout_gravity="center"
            android:padding="5dp"
            android:background="@drawable/bg_dropdown"
            android:entries="@array/oneAndTwo"
            android:gravity="center"
            android:spinnerMode="dropdown" />

        <TableRow
            android:padding="10dp"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="5dp"
                android:gravity="start"
                android:text="Risk Reduction plan developed"
                android:textColor="#000"
                />


        </TableRow>

        <android.support.v7.widget.AppCompatSpinner
            android:id="@+id/postTest5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_gravity="center"
            android:textAlignment="center"
            android:entries="@array/oneAndTwo"
            android:padding="5dp"
            android:gravity="center"
            android:background="@drawable/bg_dropdown"
            android:spinnerMode="dropdown" />

        <TableRow
            android:padding="10dp"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="5dp"
                android:gravity="start"
                android:text="Post test disclosure plan developed"
                android:textColor="#000"
                />


        </TableRow>

        <android.support.v7.widget.AppCompatSpinner
            android:id="@+id/postTest6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:textAlignment="center"
            android:layout_gravity="center"
            android:entries="@array/oneAndTwo"
            android:padding="5dp"
            android:gravity="center"
            android:background="@drawable/bg_dropdown"
            android:spinnerMode="dropdown" />

        <TableRow
            android:padding="10dp"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="5dp"
                android:gravity="start"
                android:text="Will bring partner(s) for HIV testing"
                android:textColor="#000"
                />


        </TableRow>

        <android.support.v7.widget.AppCompatSpinner
            android:id="@+id/postTest7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:textAlignment="center"
            android:layout_gravity="center"
            android:padding="5dp"
            android:background="@drawable/bg_dropdown"
            android:entries="@array/oneAndTwo"
            android:gravity="center"
            android:spinnerMode="dropdown" />





        <TableRow
            android:padding="10dp"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="5dp"
                android:gravity="start"
                android:text="Will bring own children &lt; 5 years for HIV testing"
                android:textColor="#000"
                />

        </TableRow>


        <android.support.v7.widget.AppCompatSpinner
            android:id="@+id/postTest8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_gravity="center"
            android:padding="5dp"
            android:entries="@array/oneAndTwo"
            android:gravity="center"
            android:background="@drawable/bg_dropdown"
            android:textAlignment="center"
            android:spinnerMode="dropdown" />
        <TableRow
            android:padding="10dp"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="5dp"
                android:gravity="start"
                android:text="Clients Provided with information on Family Planning and dual contraception"
                android:textColor="#000"
                />


        </TableRow>

        <android.support.v7.widget.AppCompatSpinner
            android:id="@+id/postTest9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_gravity="center"
            android:textAlignment="center"
            android:background="@drawable/bg_dropdown"
            android:padding="5dp"
            android:entries="@array/oneAndTwo"
            android:gravity="center"
            android:spinnerMode="dropdown" />
        <TableRow
            android:padding="10dp"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="5dp"
                android:gravity="start"
                android:text="Client/partner use Family Planning methods (other than condom)"
                android:textColor="#000"
                />

        </TableRow>


        <android.support.v7.widget.AppCompatSpinner
            android:id="@+id/postTest10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:background="@drawable/bg_dropdown"
            android:layout_gravity="center"
            android:padding="5dp"
            android:entries="@array/oneAndTwo"
            android:textAlignment="center"
            android:gravity="center"
            android:spinnerMode="dropdown" />
        <TableRow
            android:padding="10dp"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="5dp"
                android:gravity="start"
                android:text="Client/partner use Condom as (one) Family Planning method"
                android:textColor="#000"
                />


        </TableRow>

        <android.support.v7.widget.AppCompatSpinner
            android:id="@+id/postTest11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_gravity="center"
            android:padding="5dp"
            android:entries="@array/oneAndTwo"
            android:background="@drawable/bg_dropdown"
            android:gravity="center"
            android:textAlignment="center"
            android:spinnerMode="dropdown" />



        <TableRow
            android:padding="10dp"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="5dp"
                android:gravity="start"
                android:text="Correct condom use demostrated"
                android:textColor="#000"
                />


        </TableRow>

        <android.support.v7.widget.AppCompatSpinner
            android:id="@+id/postTest12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_gravity="center"
            android:padding="5dp"
            android:background="@drawable/bg_dropdown"
            android:entries="@array/oneAndTwo"
            android:gravity="center"
            android:textAlignment="center"
            android:spinnerMode="dropdown" />
        <TableRow
            android:padding="10dp"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="5dp"
                android:gravity="start"
                android:text="Condom provided to client"
                android:textColor="#000"
                />


        </TableRow>

        <android.support.v7.widget.AppCompatSpinner
            android:id="@+id/postTest13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_gravity="center"
            android:entries="@array/oneAndTwo"
            android:gravity="center"
            android:background="@drawable/bg_dropdown"
            android:padding="5dp"
            android:textAlignment="center"
            android:spinnerMode="dropdown" />
        <TableRow
            android:padding="10dp"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="5dp"
                android:gravity="start"
                android:text="Client referred for other services"
                android:textColor="#000"
                />



        </TableRow>
        <android.support.v7.widget.AppCompatSpinner
            android:id="@+id/postTest14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_gravity="center"
            android:background="@drawable/bg_dropdown"
            android:padding="5dp"
            android:entries="@array/oneAndTwo"
            android:gravity="center"
            android:textAlignment="center"
            android:spinnerMode="dropdown" />


    </TableLayout>
    </android.support.v7.widget.CardView>
</ScrollView>